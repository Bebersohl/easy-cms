{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ],
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "jsRules": {
    "no-console": false
  },
  "rulesDirectory": []
}
